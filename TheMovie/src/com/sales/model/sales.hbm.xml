<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.sales.model.SalesVO" table="sales" >
        
        <id name="salesID" column="salesID">
            <generator class="identity"/>
        </id>
        
        <property name="salesName" column="salesName" />
        <property name="account" column="account" />
        <property name="password" column="password" />
        <property name="theaID" column="theaID" />
        <property name="activation" column="activation" />
        
<!--         <many-to-one name="deptVO" class="com.dept.model.DeptVO" lazy="false">
            <column name="deptno" />
        </many-to-one> -->
        
    </class>
</hibernate-mapping>

